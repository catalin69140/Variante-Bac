SUBIECTUL II

5.#include <iostream>
using namespace std;

int v[25][25];

int main()
{
    int n,i,j,k=1;
    cin>>n;
    for(i=1; i<=n; i++)
    {
        for(j=1; j<=n; j++)
        {
            v[i][j]=max(i,j);
        }
    }
    for(i=1; i<=n; i++)
    {
        for(j=1; j<=n; j++)
        {
            cout<<v[i][j]<<" ";
        }
        cout<<'\n';
    }
    return 0;
}
//5
