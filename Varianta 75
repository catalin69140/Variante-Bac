SUBIECTUL II

5.#include <iostream>
using namespace std;

int v[105][105];

int main()
{
    int n,i,j,k;
    cin>>n;
    for(i=1; i<=n; i++)
    {
        k=i;
        for(j=1; j<=n; j++)
        {
            v[i][j]=(k+1)*(k+1);
            k++;
        }
    }
    for(i=1; i<=n; i++)
    {
        for(j=1; j<=n; j++)
        {
            cout<<v[i][j]<<" ";
        }
        cout<<'\n';
    }
    return 0;
}
//4
